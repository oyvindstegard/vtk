        </entry>
        <entry key="http://www.uio.no/xsd/arena/generell/v00./generell.xsd">
          <bean parent="abstractXPathEvaluator">
            <property name="expression" value="/generell/tittel" />
          </bean>
        </entry>
        <entry key="http://www.uio.no/xsd/arena/nyhet/v00./nyhet.xsd">
          <bean parent="abstractXPathEvaluator">
