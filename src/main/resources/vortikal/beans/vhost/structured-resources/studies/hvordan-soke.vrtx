view {
    ##
    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
    <html xmlns="http://www.w3.org/1999/xhtml">
      <head>
        <title></title>
      </head>
      <body id="vrtx-structured-article">
        <div id="vrtx-content">
        
        [def document structured-document()]
        [def regelverk-felles document.properties.regelverk-felles]
        [def regelverk-fri document.properties.regelverk-fri]
          <div id="vrtx-main-content" class="vrtx-hide-additional-content-false">      
            <h1>[localized 'title']</h1>
            <h2>[localized 'opptak']</h2>
            [call "comp:vis-fellestekst" {"docType":"hvordan-soke", "propName":"opptak-frist-frekvens"}]
            [call "comp:frist-frekvens-admin"]
            [call "comp:vis-fellestekst" {"docType":"hvordan-soke", "propName":"opptak-metode"}]
            [call "comp:metode"]
            [call "comp:internasjonale-sokere"]
            [call "comp:nordiske-sokere"]
            [call "comp:studiekode"]
            [call "comp:studieplasser"]
            [call "comp:pris"]
            <h2>[localized 'opptakskrav-view']</h2>
            [call "comp:vis-fellestekst" {"docType":"hvordan-soke", "propName":"opptakskrav-felles"}]
            [call "comp:opptakskrav"]
            [call "comp:generelle"]
            [call "comp:spesielle"]
            [call "comp:politiattest"]
            [call "comp:rangering-sokere"]
            <h3>[localized 'poenggrenser']</h3>
            [call "comp:vis-fellestekst" {"docType":"hvordan-soke", "propName":"forstevitnemal-felles"}]
            [call "comp:forstevitnemal-kvote"]
            [call "comp:vis-fellestekst" {"docType":"hvordan-soke", "propName":"ordinar-kvote-alle-kvalifiserte-felles"}]
            [call "comp:ordinar-kvote-alle-kvalifiserte"]
            [call "comp:vis-fellestekst" {"docType":"hvordan-soke", "propName":"poenggrenser-poengberegning-felles"}]
            [call "comp:innpassing-tidl-utdanning-admin"]
          </div>
          [if ((regelverk-fri != null && regelverk-fri != "") || (regelverk-felles != null && regelverk-felles != ""))]
            <div id="vrtx-additional-content">
              <div id="vrtx-related-content">
                [if (regelverk-felles != null && regelverk-felles != "")]
                  
                [endif]
                [call "comp:regelverk-in-additional-column"] 
              </div> 
          </div>
          [endif]
        </div>
      </body>
    </html>
    ##
  }

