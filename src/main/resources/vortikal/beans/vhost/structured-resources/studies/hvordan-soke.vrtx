          [call "comp:vis-fellestekst" {"docType":"hvordan-soke", "propName":"opptak-nordiske-sokere"}]
          [if nordiske-sokere-fri != null && nordiske-sokere-fri != ""]
          <div class="nordiske-sokere-fri">[val nordiske-sokere-fri # unescaped 'flattened']</div>
          [endif]
        [endif]
      ##  
    }
     studiekode {
      ##
