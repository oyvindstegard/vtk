        [def eksamensspraak-text-field resource-prop-obj-val(".", "eksamensspraak-text-field")]
        [if (eksamensspraak-text-field != null && eksamensspraak-text-field != "") || (eksamensspraak != null && eksamensspraak != "" && eksamensspraak != "ingen-fellestekst")]
