    teachingsemester-particular-semester:term (dropdown),
    teachingsemester-particular-semester:year (dropdown),
    teachingsemester-every-other-semester-term (dropdown),
    teachingsemester-every-other-semester-startyear (dropdown),
    group teachingsemester-every-other-semester (teachingsemester-every-other-semester-term, teachingsemester-every-other-semester-startyear) (after teachingsemester-particular-semester) horizontal,
    examsemester (dropdown),
    examsemester-particular-semester (class[if-examsemester-particular]),
    examsemester-particular-semester:term (dropdown),
    examsemester-particular-semester:year (dropdown),
    examsemester-every-other-semester-term (dropdown),
    examsemester-every-other-semester-startyear (dropdown),
    group examsemester-every-other-semester (examsemester-every-other-semester-term, examsemester-every-other-semester-startyear) (after examsemester-particular-semester) horizontal,
    level (dropdown),
