    examsemester-every-other-semester-startyear : string,
    examsemester-other : simple_html noextract,
    level : string,
    course-fee : boolean defaultvalue("false"),
    course-fee-amount : string,
    course-content : simple_html,
    learning-outcomes : simple_html,
    opptak-og-adgang-text-field : simple_html noextract,
    opptak-og-adgang : string,
    ikke-privatist-text-field : simple_html noextract,
    ikke-privatist : string
