  view {
    ##
    [def course-code resource-prop-obj-val(".", "courseContext.fs-course-code")]
    [def course-name resource-prop-obj-val(".", "courseContext.course-name")]
    [def course-content resource-prop-obj-val(".", "course-content")]
    [def learning-outcomes resource-prop-obj-val(".", "learning-outcomes")]
    [def studinfo-kontakt resource-prop-obj-val(".", "studinfo-kontakt")]
    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
    <html xmlns="http://www.w3.org/1999/xhtml">
      <head>
        <title>[if course-code != null][val course-code][endif][if course-code != null && course-name != null] - [endif][if course-name != null][val course-name][endif]</title>
      </head>
      <body id="vrtx-course">
        <table id="vrtx-academics-course-navigation" align="right" cellpadding="1" cellspacing="0">
          <tbody>
            <tr>
              <td class="navigasjon">
                [call "comp:academics-course-navigation"]
              </td>
            </tr>
          </tbody>
        </table>
        [call "comp:statusmessage"]
        <h1>[if course-code != null][val course-code][endif][if course-code != null && course-name != null] - [endif][if course-name != null][val course-name][endif]</h1>
        <div id="vrtx-academics-course-toc" style="padding-top:5px;">
          [call "comp:academics-course-toc"]
        </div>
        <table id="vrtx-academics-course-table" align="right" cellpadding="0" cellspacing="0" width="50%">
          <tbody>
            <tr>
              <td>
                [call "comp:facts"]
              </td>
            </tr>
            [if studinfo-kontakt != null && studinfo-kontakt != "" && studinfo-kontakt != "ingen-fellestekst"]
              <tr>
                <td>
                  [call "comp:vis-fellestekst" {"docType":"studinfo-kontakt", "propName":"studinfo-kontakt"}]
                </td>
              </tr>
            [endif]
            <tr class="vrtx-academics-course-table-title">
              <td>
                [call "comp:semesters"]
              </td>
            </tr>
          </tbody>
        </table>
        <div>
          [if course-content != null && course-content != ""]
            <div id="course-content">
              [val course-content # unescaped]
            </div>
          [endif]
          [if learning-outcomes != null && learning-outcomes != ""]
            <div id="learning-outcomes">
              [val learning-outcomes # unescaped]
            </div>
          [endif]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"opptak-og-adgang"}]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"ikke-privatist"}]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"obligatoriske-forkunnskaper"}]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"tidsrom-for-undervisning"}]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"adgang"}]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"vurdering-og-eksamen"}]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"hjelpemidler"}]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"eksamensspraak"}]
          [call "comp:vis-fellestekst" {"docType":"emne", "propName":"sensur"}]
