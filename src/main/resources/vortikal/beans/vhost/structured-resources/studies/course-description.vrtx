          [else]
            [def put-dash ""]
          [endif]
          <a href="#[val adgang]" class="vrtx-link-check">[localized adgang]</a>
        [endif]
        [def vurdering-og-eksamen resource-prop-obj-val(".", "vurdering-og-eksamen")]
        [def vurdering-og-eksamen-text-field resource-prop-obj-val(".", "vurdering-og-eksamen-text-field")]
        [if (vurdering-og-eksamen-text-field != null && vurdering-og-eksamen-text-field != "") || (vurdering-og-eksamen != null && vurdering-og-eksamen != "" && vurdering-og-eksamen != "ingen-fellestekst")]
          [if put-dash != null]
