resourcetype course-description : fellestekst-document {

  properties {
    credits : string,
    teachingsemester : string,
    teachingsemester-particular-semester : string,
    teachingsemester-other : simple_html,
    examsemester : string,
    examsemester-particular-semester : string,
    examsemester-other : simple_html,
    course-type : string,
    course-content : simple_html,
    learning-outcomes : simple_html,
    opptak-og-adgang-text-field : simple_html,
    opptak-og-adgang : string,
    ikke-privatist-text-field : simple_html,
    ikke-privatist : string,
    obligatoriske-forkunnskaper-text-field : simple_html,
    obligatoriske-forkunnskaper : string,
    tidsrom-for-undervisning-text-field : simple_html,
    tidsrom-for-undervisning : string,
    adgang-text-field : simple_html,
    adgang : string,
    vurdering-og-eksamen-text-field : simple_html,
    vurdering-og-eksamen : string,
    hjelpemidler-text-field : simple_html,
    hjelpemidler : string,
    eksamensspraak-text-field : simple_html,
    eksamensspraak : string,
    sensur-text-field : simple_html,
    sensur : string,
    tilsynssensor-text-field : simple_html,
    tilsynssensor : string,
    klage-text-field : simple_html,
    klage : string,
    ny-utsatt-eksamen-text-field : simple_html,
    ny-utsatt-eksamen : string,
    antall-forsok-trekk-text-field : simple_html,
    antall-forsok-trekk : string,
    tilrettelagt-eksamen-text-field : simple_html,
    tilrettelagt-eksamen : string,
    studinfo-kontakt-text-field : simple_html,
    studinfo-kontakt : string
  }

  edit-rules {
    credits (dropdown),
    teachingsemester (dropdown),
    teachingsemester-particular-semester (class[if-teachingsemester-bestemt]),
    teachingsemester-other (class[if-teachingsemester-other]),
    examsemester (dropdown),
    examsemester-particular-semester (class[if-examsemester-bestemt]),
    examsemester-other (class[if-examsemester-other]),
    course-type (dropdown),
    opptak-og-adgang (class[vrtx-shared-text]),
    group opptak-og-adgang-group (opptak-og-adgang-text-field, opptak-og-adgang) vertical,
    ikke-privatist (class[vrtx-shared-text]),
    group ikke-privatist-group (ikke-privatist-text-field, ikke-privatist) vertical,
    obligatoriske-forkunnskaper (class[vrtx-shared-text]),
    group obligatoriske-forkunnskaper-group (obligatoriske-forkunnskaper-text-field, obligatoriske-forkunnskaper) vertical,
    tidsrom-for-undervisning (class[vrtx-shared-text]),
    group tidsrom-for-undervisning-group (tidsrom-for-undervisning-text-field, tidsrom-for-undervisning) vertical,
    adgang (class[vrtx-shared-text]),
    group adgang-group (adgang-text-field, adgang) vertical,
    vurdering-og-eksamen (class[vrtx-shared-text]),
    group vurdering-og-eksamen-group (vurdering-og-eksamen-text-field, vurdering-og-eksamen) vertical,
    hjelpemidler (class[vrtx-shared-text]),
    group hjelpemidler-group (hjelpemidler-text-field, hjelpemidler) vertical,
    eksamensspraak (class[vrtx-shared-text]),
    group eksamensspraak-group (eksamensspraak-text-field, eksamensspraak) vertical,
    sensur (class[vrtx-shared-text]),
    group sensur-group (sensur-text-field, sensur) vertical,
    tilsynssensor (class[vrtx-shared-text]),
    group tilsynssensor-group (tilsynssensor-text-field, tilsynssensor) vertical,
    klage (class[vrtx-shared-text]),
    group klage-group (klage-text-field, klage) vertical,
    ny-utsatt-eksamen (class[vrtx-shared-text]),
    group ny-utsatt-eksamen-group (ny-utsatt-eksamen-text-field, ny-utsatt-eksamen) vertical,
    antall-forsok-trekk (class[vrtx-shared-text]),
    group antall-forsok-trekk-group (antall-forsok-trekk-text-field, antall-forsok-trekk) vertical,
    tilrettelagt-eksamen (class[vrtx-shared-text]),
    group tilrettelagt-eksamen-group (tilrettelagt-eksamen-text-field, tilrettelagt-eksamen) vertical,
    studinfo-kontakt (class[vrtx-shared-text]),
    group studinfo-kontakt-group (studinfo-kontakt, studinfo-kontakt-text-field) vertical
  }

  view-components {

    facts {
        ##
        [def credits resource-prop(".", "credits")]
        [def creditsdecimal resource-prop-obj-val(".", "creditsdecimal")]
        [def content-locale resource-prop-obj-val(".", "contentLocale")]
        [def teachingsemester resource-prop-obj-val(".", "teachingsemester")]
        [def teachingsemester-particular-semester resource-prop-obj-val(".", "teachingsemester-particular-semester")]
        [def teachingsemester-other resource-prop-obj-val(".", "teachingsemester-other")]
        [def examsemester resource-prop-obj-val(".", "examsemester")]
        [def examsemester-particular-semester resource-prop-obj-val(".", "examsemester-particular-semester")]
        [def examsemester-other resource-prop-obj-val(".", "examsemester-other")]
        <table class="course" cellpadding="0" cellspacing="0" width="100%">
          <tbody>
            <tr class="vrtx-academics-course-table-title">
              <td colspan="2"><b>[localized 'facts']:</b></td>
            </tr>
            <tr valign="top">
              <td>[localized 'credits']:</td>
              <td>[if credits != null][val credits][if creditsdecimal != null && creditsdecimal != "" && creditsdecimal != "0"],[val creditsdecimal][endif][endif]</td>
            </tr>
            <tr valign="top">
              <td>[localized 'teachingsemester']:</td>
              [if teachingsemester != null && teachingsemester = "particular-semester"]
                <td>[if teachingsemester-particular-semester != null][val teachingsemester-particular-semester][endif]</td>
              [elseif teachingsemester != null && teachingsemester = "other"]
                <td>[val teachingsemester-other # unescaped]</td>
              [else]
                <td>[if teachingsemester != null][localized teachingsemester][endif]</td>
              [endif]
            </tr>
            <tr valign="top">
              <td>[localized 'examsemester']:</td>
              [if examsemester != null && examsemester = "particular-semester"]
                <td>[if examsemester-particular-semester != null][val examsemester-particular-semester][endif]</td>
              [elseif examsemester != null && examsemester = "other"]
                <td>[val examsemester-other # unescaped]</td>
              [else]
                <td>[if examsemester != null][localized examsemester][endif]</td>
              [endif]
            </tr>
            <tr valign="top">
              <td>[localized 'content-locale']:</td>
              [if content-locale != null && content-locale = "no_NO"]
                <td>[localized 'locale-no']</td>
              [elseif content-locale != null && content-locale = "en"]
                <td>[localized 'locale-en']</td>
              [elseif content-locale != null && content-locale = "no_NO_NY"]
                <td>[localized 'locale-nn']</td>
              [endif]
            </tr>
            <tr valign="top">
              <td>[localized 'administrated']:</td>
              <td><a href="http://www.mn.uio.no/ifi/" class="vrtx-link-check">Institutt for informatikk (Ifi)</a></td>
            </tr>
          </tbody>
        </table>
        ##
    }

    semesters {
        ##
        <table class="master" cellpadding="5" cellspacing="5" width="100%">
          <tbody>
            <tr>
              <td>[localized 'semester']
                <ul>
                  [def uri resource-prop("./", "uri")]
                  [def semesters search('uri=' + uri + '/* && type IN semester-context')]
                  [if typeof(semesterSearchSort) = 'function']
                    [def semesters semesterSearchSort(semesters)]
                  [endif] 
                  [list semesters semester]
                    [def url view-url(semester)]
                    [def title resource-prop(semester, "semesterContext.semester")]
                    <li>
                      <a href="[val url]">[if title != null && title != ""][val title][else][val url][endif]</a>
                    </li>
                  [endlist]
                </ul>
              </td>
            </tr>
          </tbody>
        </table>
        ##
    }

    academics-course-navigation {
        ##
        <div class="academics-course-navigation">
          [def this-uri resource-prop(".", "uri")]
          [def parent-uri resource-prop("./", "uri")]
          [def content-locale resource-prop-obj-val(".", "contentLocale")]
          [if content-locale != null && (content-locale = "no_NO" || content-locale = "no_NO_NY")]
            [def resource resource(parent-uri + '/index-eng.html')]
            [if resource != null]
              [def resource-uri resource-prop(resource, "uri")]
              [def resource-content-locale resource-prop-obj-val(resource, "contentLocale")]
              [if resource-uri != this-uri && resource-content-locale = "en"]
                [def view-eng view-url(resource)]
                <a href="[val view-eng]" class="vrtx-link-check">[localized 'version-en']</a>
              [endif]
            [endif]
          [elseif content-locale != null && content-locale = "en"]
            [def resource resource(parent-uri + '/index.html')]
            [if resource != null]
              [def resource-uri resource-prop(resource, "uri")]
              [def resource-content-locale resource-prop-obj-val(resource, "contentLocale")]
              [if resource-uri != this-uri && (resource-content-locale = "no_NO" || resource-content-locale = "no_NO_NY")]
                [def view-no view-url(resource)]
                <a href="[val view-no]" class="vrtx-link-check">[localized 'version-no']</a>
              [endif]
            [endif]
          [endif]
        </div>
        ##
    }

    academics-course-toc {
        ##
        [def put-dash null]
        [def course-content resource-prop-obj-val(".", "course-content")]
        [if course-content != null && course-content != ""]
          [def put-dash ""]
          <a href="#course-content" class="vrtx-link-check">[localized 'course-content']</a>
        [endif]
        [def learning-outcomes resource-prop-obj-val(".", "learning-outcomes")]
        [if learning-outcomes != null && learning-outcomes != ""]
          [if put-dash != null]
            -
          [else]
            [def put-dash ""]
          [endif]
          <a href="#learning-outcomes" class="vrtx-link-check">[localized 'learning-outcomes']</a>
        [endif]
        [def opptak-og-adgang resource-prop-obj-val(".", "opptak-og-adgang")]
        [def opptak-og-adgang-text-field resource-prop-obj-val(".", "opptak-og-adgang-text-field")]
        [if (opptak-og-adgang-text-field != null && opptak-og-adgang-text-field != "") || (opptak-og-adgang != null && opptak-og-adgang != "" && opptak-og-adgang != "ingen-fellestekst")]
          [if put-dash != null]
            -
          [else]
            [def put-dash ""]
          [endif]
          <a href="#[val opptak-og-adgang]" class="vrtx-link-check">[localized opptak-og-adgang]</a>
        [endif]
        [def ikke-privatist resource-prop-obj-val(".", "ikke-privatist")]
        [def ikke-privatist-text-field resource-prop-obj-val(".", "ikke-privatist-text-field")]
        [if (ikke-privatist-text-field != null && ikke-privatist-text-field != "") || (ikke-privatist != null && ikke-privatist != "" && ikke-privatist != "ingen-fellestekst")]
          [if put-dash != null]
            -
          [else]
            [def put-dash ""]
          [endif]
          <a href="#[val ikke-privatist]" class="vrtx-link-check">[localized ikke-privatist]</a>
        [endif]
        [def obligatoriske-forkunnskaper resource-prop-obj-val(".", "obligatoriske-forkunnskaper")]
        [def obligatoriske-forkunnskaper-text-field resource-prop-obj-val(".", "obligatoriske-forkunnskaper-text-field")]
        [if (obligatoriske-forkunnskaper-text-field != null && obligatoriske-forkunnskaper-text-field != "") || (obligatoriske-forkunnskaper != null && obligatoriske-forkunnskaper != "" && obligatoriske-forkunnskaper != "ingen-fellestekst")]
          [if put-dash != null]
