        [def tidsrom-for-undervisning-text-field resource-prop-obj-val(".", "tidsrom-for-undervisning-text-field")]
        [if (tidsrom-for-undervisning-text-field != null && tidsrom-for-undervisning-text-field != "") || (tidsrom-for-undervisning != null && tidsrom-for-undervisning != "" && tidsrom-for-undervisning != "ingen-fellestekst")]
          [if put-dash != null]
