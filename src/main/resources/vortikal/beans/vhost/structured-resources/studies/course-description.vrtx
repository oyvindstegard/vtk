          [endif]
          <a href="#[val tidsrom-for-undervisning]" class="vrtx-link-check">[localized tidsrom-for-undervisning]</a>
        [endif]
        [def adgang resource-prop-obj-val(".", "adgang")]
        [def adgang-text-field resource-prop-obj-val(".", "adgang-text-field")]
        [if (adgang-text-field != null && adgang-text-field != "") || (adgang != null && adgang != "" && adgang != "ingen-fellestekst")]
          [if put-dash != null]
