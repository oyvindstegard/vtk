  view-components {

    facts {
        ##
        [def credits resource-prop(".", "credits")]
        [def level resource-prop-obj-val(".", "level")]
        [def creditsdecimal resource-prop-obj-val(".", "creditsdecimal")]
        [def content-locale resource-prop-obj-val(".", "contentLocale")]
        [def teachingsemester resource-prop-obj-val(".", "teachingsemester")]
        [def teachingsemester-other resource-prop-obj-val(".", "teachingsemester-other")]
        [def examsemester resource-prop-obj-val(".", "examsemester")]
        [def examsemester-other resource-prop-obj-val(".", "examsemester-other")]
        [def course-fee resource-prop-obj-val(".", "course-fee")]
        [def course-fee-amount resource-prop-obj-val(".", "course-fee-amount")]
        <table class="course" cellpadding="0" cellspacing="0" width="100%">
          <tbody>
            <tr class="vrtx-academics-course-table-title">
              <td colspan="2"><b>[localized 'facts']:</b></td>
            </tr>
            <tr valign="top">
              <td>[localized 'credits']:</td>
              <td>[if credits != null][val credits][if creditsdecimal != null && creditsdecimal != "" && creditsdecimal != "0"],[val creditsdecimal][endif][endif]</td>
            </tr>
            [if level != null && level != "" && level != "not-set"]
              <tr valign="top">
                <td>[localized 'level']:</td>
                <td>[localized level]</td>
              </tr>
            [endif]
            <tr valign="top">
              <td>[localized 'teachingsemester']:</td>
              <td>
                [if teachingsemester != null && teachingsemester = "particular-semester"]
                  [def document structured-document()]
                  [def teachingsemesters document.properties.teachingsemester-particular-semester]
                  [if teachingsemesters != null]
                    [list teachingsemesters teachingsemester]
                      [if teachingsemester.term != null && teachingsemester.year != null]
                        [def term teachingsemester.term]
                        <p>[localized term] [val teachingsemester.year]</p>
                      [endif]
                    [endlist]
                  [endif]
                [elseif teachingsemester != null && teachingsemester = "every-other"]
                  [def term 'every-other-' + resource-prop-obj-val(".", "teachingsemester-every-other-semester-term")]
                  [def startyear resource-prop-obj-val(".", "teachingsemester-every-other-semester-startyear")]
                  [if term != null && startyear != null]
                    <p>[localized term]</p>
                    <p>[localized 'every-other-start'] [val startyear] [localized 'teaching-every-other-end']</p>
                  [endif]
                [else]
                  [if teachingsemester != null][localized teachingsemester][endif]
                [endif]
                [if teachingsemester-other != null][val teachingsemester-other # unescaped][endif]
              </td>
            </tr>
            <tr valign="top">
              <td>[localized 'examsemester']:</td>
              <td>
                [if examsemester != null && examsemester = "particular-semester"]
                  [def document structured-document()]
                  [def examsemesters document.properties.examsemester-particular-semester]
                  [if examsemesters != null]
                    [list examsemesters examsemester]
                      [if examsemester.term != null && examsemester.year != null]
                        [def term examsemester.term]
                        <p>[localized term] [val examsemester.year]</p>
                      [endif]
                    [endlist]
                  [endif]
                [elseif examsemester != null && examsemester = "every-other"]
                  [def term 'every-other-' + resource-prop-obj-val(".", "examsemester-every-other-semester-term")]
                  [def startyear resource-prop-obj-val(".", "examsemester-every-other-semester-startyear")]
                  [if term != null && startyear != null]
                    <p>[localized term]</p>
                    <p>[localized 'every-other-start'] [val startyear] [localized 'exam-every-other-end']</p>
                  [endif]
                [else]
                  [if examsemester != null][localized examsemester][endif]
                [endif]
                [if examsemester-other != null][val examsemester-other # unescaped][endif]
              </td>
            </tr>
            <tr valign="top">
              <td>[localized 'content-locale']:</td>
              [if content-locale != null && content-locale = "no_NO"]
                <td>[localized 'locale-no']</td>
              [elseif content-locale != null && content-locale = "en"]
                <td>[localized 'locale-en']</td>
              [elseif content-locale != null && content-locale = "no_NO_NY"]
                <td>[localized 'locale-nn']</td>
              [endif]
            </tr>
            [if course-fee != null && course-fee && course-fee-amount != null && course-fee-amount != ""]
              <tr valign="top">
                <td>[localized 'course-fee']:</td>
                <td>[val course-fee-amount]</td>
              </tr>
            [endif]
          </tbody>
        </table>
        ##
    }

    semesters {
