          [endif]
          <a href="#[val tidsrom-for-undervisning]" class="vrtx-link-check">[localized tidsrom-for-undervisning]</a>
        [endif]
        [def adgang resource-prop-obj-val(".", "adgang")]
        [def adgang-text-field resource-prop-obj-val(".", "adgang-text-field")]
        [if (adgang-text-field != null && adgang-text-field != "") || (adgang != null && adgang != "" && adgang != "ingen-fellestekst")]
          [if put-dash != null]
            -
          [else]
            [def put-dash ""]
          [endif]
          <a href="#[val adgang]" class="vrtx-link-check">[localized adgang]</a>
        [endif]
        [def vurdering-og-eksamen resource-prop-obj-val(".", "vurdering-og-eksamen")]
        [def vurdering-og-eksamen-text-field resource-prop-obj-val(".", "vurdering-og-eksamen-text-field")]
        [if (vurdering-og-eksamen-text-field != null && vurdering-og-eksamen-text-field != "") || (vurdering-og-eksamen != null && vurdering-og-eksamen != "" && vurdering-og-eksamen != "ingen-fellestekst")]
          [if put-dash != null]
            -
          [else]
            [def put-dash ""]
          [endif]
          <a href="#[val vurdering-og-eksamen]" class="vrtx-link-check">[localized vurdering-og-eksamen]</a>
        [endif]
        [def hjelpemidler resource-prop-obj-val(".", "hjelpemidler")]
        [def hjelpemidler-text-field resource-prop-obj-val(".", "hjelpemidler-text-field")]
        [if (hjelpemidler-text-field != null && hjelpemidler-text-field != "") || (hjelpemidler != null && hjelpemidler != "" && hjelpemidler != "ingen-fellestekst")]
