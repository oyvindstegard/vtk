        [def hjelpemidler-text-field resource-prop-obj-val(".", "hjelpemidler-text-field")]
        [if (hjelpemidler-text-field != null && hjelpemidler-text-field != "") || (hjelpemidler != null && hjelpemidler != "" && hjelpemidler != "ingen-fellestekst")]
