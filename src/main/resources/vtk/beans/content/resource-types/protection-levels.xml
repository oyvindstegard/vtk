<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

  <bean id="PROTECTION_LEVEL_ACL_WRITE"
        class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean">
    <property name="staticField"
              value="vtk.repository.resourcetype.PropertyType.PROTECTION_LEVEL_ACL_WRITE" />
  </bean>

  <bean id="PROTECTION_LEVEL_ACL_ADMIN"
        class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean">
    <property name="staticField"
              value="vtk.repository.resourcetype.PropertyType.PROTECTION_LEVEL_ACL_ADMIN" />
  </bean>

  <bean id="PROTECTION_LEVEL_ROLE_ADMIN"
        class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean">
    <property name="staticField"
              value="vtk.repository.resourcetype.PropertyType.PROTECTION_LEVEL_ROLE_ADMIN" />
  </bean>

  <bean id="PROTECTION_LEVEL_ROLE_ROOT"
        class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean">
    <property name="staticField"
              value="vtk.repository.resourcetype.PropertyType.PROTECTION_LEVEL_ROLE_ROOT" />
  </bean>


  <bean id="PROTECTION_LEVEL_UNEDITABLE"
        class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean">
    <property name="staticField"
              value="vtk.repository.resourcetype.PropertyType.PROTECTION_LEVEL_UNEDITABLE" />
  </bean>

</beans>
