<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
  <!-- Global Ehcache config for Ehcache manager -->
  
  <!-- Individual caches are configured using Spring's EhCacheFactoryBean. -->
  <!-- See also http://ehcache.org/documentation/configuration.html -->

  <diskStore path="java.io.tmpdir/vortikal/ehcache" />

  <defaultCache
    maxElementsInMemory="100"
    maxElementsOnDisk="10000"
    eternal="false"
    overflowToDisk="true"
    timeToIdleSeconds="120"
    timeToLiveSeconds="120" />

</ehcache>
