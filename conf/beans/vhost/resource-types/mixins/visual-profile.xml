      <list>
        <bean class="org.vortikal.repository.resourcetype.Value">
          <constructor-arg type="boolean">
            <value>false</value>
          </constructor-arg>
        </bean>
      </list>
    </property>
    <property name="defaultValue">
        <bean class="org.vortikal.repository.resourcetype.Value">
          <constructor-arg type="boolean">
            <value>false</value>
