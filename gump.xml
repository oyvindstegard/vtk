<?xml version="1.0" encoding="UTF-8"?>

<module name="vortikal">
  <description>The Vortikal Web Framework</description>
  <url href="http://vortikal.berlios.de">
  </url>
  <cvs repository="">
  </cvs>
  <project name="vortikal">
    <maven goal="jar">
      <property name="final.name" value="vortikal-@@DATE@@">
      </property>
    </maven>
    <package>org.vortikal</package>
    <depend project="aopalliance">
    </depend>
    <depend project="asm">
    </depend>
    <depend project="beanshell">
    </depend>
    <depend project="commons-codec">
    </depend>
    <depend project="commons-fileupload">
    </depend>
    <depend project="commons-logging">
    </depend>
    <depend project="concurrent">
    </depend>
    <depend project="freemarker">
    </depend>
    <depend project="groovy">
    </depend>
    <depend project="hsqldb">
    </depend>
    <depend project="jakarta-ant">
    </depend>
    <depend project="jakarta-log4j">
    </depend>
    <depend project="jakarta-oro">
    </depend>
    <depend project="jakarta-servletapi">
    </depend>
    <depend project="jaxen">
    </depend>
    <depend project="jdom">
    </depend>
    <depend project="jug">
    </depend>
    <depend project="lucene">
    </depend>
    <depend project="postgresql">
    </depend>
    <depend project="quartz">
    </depend>
    <depend project="saxpath">
    </depend>
    <depend project="springframework">
    </depend>
    <depend project="xml-apis">
    </depend>
    <depend project="xml-xalan2">
    </depend>
    <depend project="xml-xerces">
    </depend>
    <depend project="xml-xerces2">
    </depend>
    <work nested="target/classes">
    </work>
    <home nested="target">
    </home>
    <jar name="vortikal-@@DATE@@.jar">
    </jar>
    <javadoc nested="target/docs/apidocs">
    </javadoc>
    <nag to="" from="vortikal development &lt;&gt;">
    </nag>
  </project>
</module>